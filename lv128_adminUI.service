[Unit]
Description=LVWEB-128 AdminUI

[Service]
User=deploy
Group=deploy
Environment="APPDIR=/opt/lv128/AdminUI/"
ExecStart=/bin/bash -c "source $APPDIR/bin/activate ;python $APPDIR/manage.py runserver $APPDIR/log/all_operation.log $APPDIR/log/debug.log -p 8810  "

[Install]
WantedBy=multi-user.target
